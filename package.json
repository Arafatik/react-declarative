        "@types/react-dom": "^16.9.7",
        "@types/react-window": "^1.8.5",
        "@types/sha.js": "^2.4.0",
        "dts-bundle": "^0.7.3",
        "dts-minify": "^0.3.0",
        "esbuild": "^0.14.23",
        "esbuild-node-externals": "^1.4.1",
        "jest": "^26.6.3",
        "rimraf": "^3.0.2",
